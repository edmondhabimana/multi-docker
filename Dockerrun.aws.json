{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "edmondhabimana/multi-client",
      "memory": 128,
      "essentail": false,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000
        }
      ]
    },
    {
      "name": "nginx",
      "image": "edmondhabimana/multi-nginx",
      "memory": 128,
      "essentail": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    },
    {
      "name": "api",
      "image": "edmondhabimana/multi-api",
      "memory": 128,
      "essentail": false,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000
        }
      ]
    },
    {
      "name": "worker",
      "image": "edmondhabimana/multi-worker",
      "memory": 128,
      "essentail": false
    }
  ]
}